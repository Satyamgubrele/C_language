#include<stdio.h>
#include<math.h>
 void main()
 {
     int a,rem,temp;
     int sum=0;
     printf("enter a no\n");
     scanf("%d",&a);
    temp=a;

     while(a>0)
     {
         rem=a%10;
         sum=sum+(rem*rem*rem);
         a=a/10;
        
     }
     if(temp==sum)
     printf("%d is a armstrong no",temp);
     else
     printf("%d is not armstrong no",temp);
 }
