
#include<stdio.h>
//function with no argument but a return value
int add(void);
void main()
{  
int y;

y=add();
printf("%d",y);


}
int add(void)
{
    int a,b,sum;
    scanf("%d%d",&a,&b);
     sum=a+b;
    return(sum);
    
}








